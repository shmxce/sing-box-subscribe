{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "default_mode": "rule"
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "proxy_dns",
        "type": "tls",
        "server": "8.8.8.8",
        "detour": "🚀 默认出站"
      },
      {
        "tag": "local_dns",
        "type": "https",
        "server": "223.5.5.5"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "local_dns"
      },
      {
        "clash_mode": "global",
        "server": "proxy_dns"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "geosite_cn",
        "server": "fakeip"
      },
      {
        "rule_set": "geosite_cn",
        "server": "local_dns"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite_geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip_cn"
          }
        ],
        "server": "proxy_dns",
        "client_subnet": "114.114.114.114/24"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip"
      }
    ],
    "final": "proxy_dns",
    "strategy": "prefer_ipv4",
    "independent_cache": false
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.18.0.0/30",
        "fdfe:dcba:9876::0/126"
      ],
      "auto_route": true,
      "strict_route": true,
      "mtu": 9000,
      "stack": "mixed"
    }
  ],
  "outbounds": [
    {
      "tag": "🐢 直连",
      "type": "direct"
    },
    {
      "tag": "📌 单选节点",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "剩余|流量|raffic|有效|时间|到期|xpire|地址|网址|官网|自动|最优|最快"
          ]
        }
      ],
      "interrupt_exist_connections": true
    },
    {
      "tag": "自动选择-中级低配",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🌌"
          ]
        },
        {
          "action": "include",
          "keywords": [
            "中级"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "剩余|流量|raffic|有效|时间|到期|xpire|地址|网址|官网|自动|最优|最快|TG群组"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "30m",
      "tolerance": 50,
      "interrupt_exist_connections": false
    },
    {
      "tag": "自动选择-低配",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🌌"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "剩余|流量|raffic|有效|时间|到期|xpire|地址|网址|官网|自动|最优|最快|TG群组"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "30m",
      "tolerance": 50,
      "interrupt_exist_connections": false
    },
    {
      "tag": "自动选择-高配",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🐷"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "30m",
      "tolerance": 50,
      "interrupt_exist_connections": false
    },
    {
      "tag": "自动选择-GPT可用",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "IEPL-GPT|IEPL2-GPT"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "30m",
      "tolerance": 50,
      "interrupt_exist_connections": false
    },
    {
      "tag": "自动选择-低配无锁地区",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "IEPL|打杂|香港|澳门|台湾|新加坡"
          ]
        }
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "30m",
      "tolerance": 50,
      "interrupt_exist_connections": false
    },
    {
      "tag": "🚀 默认出站",
      "type": "selector",
      "outbounds": [
        "📌 单选节点",
        "自动选择-低配",
        "自动选择-中级低配",
        "自动选择-高配",
        "自动选择-GPT可用",
        "自动选择-低配无锁地区"
      ],
      "default": "自动选择-低配",
      "interrupt_exist_connections": true
    },
    {
      "tag": "⚙️ Fallback",
      "type": "selector",
      "outbounds": [
        "📌 单选节点",
        "自动选择-低配",
        "自动选择-中级低配",
        "自动选择-高配"
      ],
      "default": "自动选择-低配",
      "interrupt_exist_connections": true
    },
    {
      "tag": "🤖 OpenAI",
      "type": "selector",
      "outbounds": [
        "自动选择-GPT可用",
        "自动选择-低配无锁地区",
        "📌 单选节点"
      ],
      "default": "自动选择-GPT可用",
      "interrupt_exist_connections": true
    }
  ],
  "route": {
    "final": "⚙️ Fallback",
    "auto_detect_interface": true,
    "override_android_vpn": true,
    "default_domain_resolver": {
      "server": "local_dns"
    },
    "rules": [
      {
        "inbound": ["tun-in"],
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules":[
          {
            "port":53
          },
          {
